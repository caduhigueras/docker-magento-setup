# MAGENTO_STORE_CODE_ES=es
# MAGENTO_STORE_URL_UK=magento.local.uk
# MAGENTO_STORE_CODE_UK=uk

#********* Nginx Settings **********/
NGINX_DOCKER_IMAGE_VERSION=latest
MAGENTO_SERVER_NAME=_
NGINX_CONF_FILE=nginx.conf.sample

#********* Varnish Settings **********/
VARNISH_DOCKER_IMAGE_VERSION=latest
VARNISH_HTTP_PORT=6081
VARNISH_BACKEND_PORT=6082
VARNISH_MEMORY_ALLOCATION=4G
VARNISH_HTTP_RESP_HDR_LEN=131072
VARNISH_HTTP_RESP_SIZE=196608
VARNISH_HTTP_WORKSPACE_BACKEND=512M

#********* PHP Settings **********/
PHP_DOCKER_IMAGE_VERSION="8.2-fpm"
PHP_INSTALL_DEPENDENCIES=libfreetype6-dev libjpeg62-turbo-dev libpng-dev libxml2-dev libonig-dev libzip-dev zip libicu-dev g++ libxslt-dev libcurl4-openssl-dev pkg-config git curl unzip redis-tools redis-server default-mysql-client sudo libgmp-dev libldap2-dev freetds-dev libpspell-dev libsodium-dev
PHP_INSTALL_EXTENSIONS="bcmath calendar ctype curl dba dom exif fileinfo filter ftp gd gettext gmp iconv intl ldap mbstring mysqli opcache pcntl pdo pdo_dblib pdo_mysql posix pspell session simplexml sockets sodium sysvmsg sysvsem sysvshm xml xmlwriter xsl zip soap gmp ldap pdo_dblib pspell sodium"
PHP_MEMORY_LIMIT=2G
PHP_MAX_EXECUTION_TIME=300
PHP_UPLOAD_MAX_FILE_SIZE=32M
PHP_POST_MAX_SIZE=32M
PHP_TIMEZONE=UTC
PHP_INSTALL_XDEBUG=true
PHP_REALPATH_CACHE_SIZE=10M
PHP_REALPATH_CACHE_TTL=7200
PHP_OPCACHE_MEMORY_CONSUMPTION=512
PHP_OPCACHE_MAX_ACCELERATED_FILES=60000
PHP_OPCACHE_CONSISTENCY_CHECKS=0
PHP_OPCACHE_VALIDATE_TIMESTAMPS=0
PHP_OPCACHE_ENABLE_CLI=1
PHP_OPCACHE_SAVE_COMMENTS=On
PHP_OPCACHE_FILE_OVERRIDE=On
PHP_OPCACHE_INTERNED_STRINGS=100
PHP_CPUS=2.0
PHP_PM=dynamic
PHP_PM_MAX_CHILDREN=5
PHP_PM_START_SERVERS=2
PHP_PM_MIN_SPARE_SERVERS=1
PHP_PM_MAX_SPARE_SERVERS=3
PHP_PM_PROCESS_IDLE_TIMEOUT=10s
BLACKFIRE_SERVER_ID=
BLACKFIRE_SERVER_TOKEN=
BLACKFIRE_CLIENT_ID=
BLACKFIRE_CLIENT_TOKEN=
INSTALL_N98=true
#XDEBUG_SERVER_NAME will typically be the first name that show in th nginx server_name directive
XDEBUG_SERVER_NAME=m2.demo

